{
  "name": "my-site",
  "version": "6.0.0",
  "description": "Evan Lovely's Site",
  "private": true,
  "author": {
    "name": "Evan Lovely",
    "url": "https://github.com/EvanLovely"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:EvanLovely/my-site.git"
  },
  "scripts": {
    "start": "export NODE_ENV=development && gulp compile && webpack-dev-server --inline --hot --content-base=dist & gulp",
    "start:prod": "export NODE_ENV=production && npm run compile:prod && webpack -p --watch & NODE_ENV=production gulp",
    "compile": "export NODE_ENV=development && webpack -d && gulp compile",
    "compile:prod": "export NODE_ENV=production && webpack -p && gulp compile",
    "clean": "rm -rf dist",
    "new:src": "yo ./_templates/index.js",
    "new:content": "yo ./scripts/newContent.js",
    "predeploy:stage": "npm run compile:prod",
    "predeploy:prod": "npm run compile:prod",
    "deploy:stage": "time rsync -azP --delete dist--prod/ evlove@evanlovely.com:/home/evlove/www/dev.evanlovely.com/public/",
    "deploy:prod": "time rsync -azP --delete dist--prod/ evlove@evanlovely.com:/home/evlove/www/evanlovely.com/public/",
    "deploy:both": "npm run compile:prod && npm run deploy:stage && npm run deploy:prod",
    "test:links:local": "blc -ro http://localhost:3042",
    "test:links:stage": "blc -ro http://dev.evanlovely.com",
    "test:links:prod": "blc -ro http://evanlovely.com",
    "test": "mocha src/**/*.test.js"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.8.0",
    "broken-link-checker": "^0.7.1",
    "cheerio": "^0.20.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "gulp-changed": "^1.3.1",
    "gulp-eslint": "^3.0.0",
    "gulp-image-resize": "^0.9.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "iframify": "github:edenspiekermann/iframify",
    "imagemin-pngquant": "^5.0.0",
    "mocha": "^2.4.5",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "request": "^2.74.0",
    "typings": "^1.3.2",
    "webpack-dev-server": "^1.14.1",
    "yeoman-generator": "^0.24.1",
    "yo": "^1.8.4"
  },
  "dependencies": {
    "babel-loader": "^6.2.4",
    "evans-sass-tools": "^1.4.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.3",
    "gray-matter": "^2.0.2",
    "gulp": "^3.9.1",
    "gulp-help": "^1.6.1",
    "history": "^3.0.0",
    "js-yaml": "^3.5.5",
    "marked": "^0.3.5",
    "modularscale-sass": "^2.1.1",
    "moment": "^2.14.1",
    "normalize.scss": "^0.1.0",
    "p2-theme-core": "^8.0.0",
    "path-parse": "^1.0.5",
    "rss": "^1.2.1",
    "typeset": "^0.2.2",
    "webpack": "^1.13.1",
    "whatwg-fetch": "^1.0.0"
  }
}
