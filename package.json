  },
  "repository": {
    "type": "git",
    "url": "git@github.com:EvanLovely/my-site.git"
  },
  "scripts": {
    "start": "export NODE_ENV=development && npm run compile && webpack --watch & gulp",
    "compile": "export NODE_ENV=development && npm run clean && webpack -d && gulp compile",
    "compile:prod": "export NODE_ENV=production && npm run clean && webpack && gulp compile",
    "clean": "rm -rf dist",
    "new": "yo ./_templates/index.js",
    "test:links": "blc -ro http://localhost:3042",
    "test": "mocha tests/*.test.js"
  },
